<!-- <form class="slide-form" action="/tutorial_slides"　method="post"  enctype="multipart/form-data"> -->
<div class="slide-form fields">
    <div class="destroy-btn">
                <%= link_to_remove_fields "削除", f %>
            </div>
    <%= f.hidden_field :tutorial_id %>
        <fieldset>
            <div id="test-2">
                <div class="slide-img">
                    <label>画像</label>
                    <%= f.file_field :image %>
                </div>
                <div class="slide-title">
                    <label>タイトル</label> 
                    <%= f.text_field :title %>
                </div>          
                <div class="slide-description">
                    <label>説明</label>
                     <%= f.text_area :body, :size=>"60x10" %> 
                </div>
            </div>
            <!-- <input type="submit"> -->
        </fieldset>
    <!-- </form> -->
</div>
